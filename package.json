    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^8.0.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
